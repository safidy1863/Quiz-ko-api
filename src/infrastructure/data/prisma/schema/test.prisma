model Test {
    id                String              @id @default(uuid())
    title             String
    duration          Int                // TODO : change this to time type 
    isActive          Boolean?            @default(true)
    subjectId         String
    subject           Subject             @relation(fields: [subjectId], references: [id])
    results           Result[]
    class             TestsClass[]
    studentTestAnswer StudentTestAnswer[]
}

enum Gender {
    MALE
    FEMALE
}

model Student {
    id                 String  @unique
    registrationNumber String  @unique         @db.VarChar(20)
    gender             Gender
    phone              String  @db.VarChar(20)
    user               User    @relation(fields: [id], references: [id])
    classId            String?
    class              Class?  @relation(fields: [classId], references: [id])
    results            Result[]
    studentTestAnswer  StudentTestAnswer[]
}
